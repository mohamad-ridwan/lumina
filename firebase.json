{
  "hosting": {
    "site": "lumina-id",
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/cached-storage-files/**",
        "destination": "/e-learning-rp.appspot.com/$1"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
